package com.company.dto;

public class EmployeeDTO {
	private String E_ID = "";
	private String E_PW = "";
	private String E_NAME = "";
	private String E_EMAIL1 = "";
	private String E_EMAIL2 = "";
	private int E_TEL1 = 0;
	private int E_TEL2 = 0;
	private int E_TEL3 = 0; 
	private String E_ENTER_DT = "";
	private String E_RESIGN_DT = "";
	private String E_RANK = "";
	private String E_AUTHORITY = "";
	
	public EmployeeDTO() {}
	
	
	
	public String getE_ID() {
		return E_ID;
	}
	public void setE_ID(String e_ID) {
		E_ID = e_ID;
	}
	public String getE_PW() {
		return E_PW;
	}
	public void setE_PW(String e_PW) {
		E_PW = e_PW;
	}
	public String getE_NAME() {
		return E_NAME;
	}
	public void setE_NAME(String e_NAME) {
		E_NAME = e_NAME;
	}
	public String getE_EMAIL1() {
		return E_EMAIL1;
	}
	public void setE_EMAIL1(String e_EMAIL1) {
		E_EMAIL1 = e_EMAIL1;
	}
	public String getE_EMAIL2() {
		return E_EMAIL2;
	}
	public void setE_EMAIL2(String e_EMAIL2) {
		E_EMAIL2 = e_EMAIL2;
	}
	public int getE_TEL1() {
		return E_TEL1;
	}
	public void setE_TEL1(int e_TEL1) {
		E_TEL1 = e_TEL1;
	}
	public int getE_TEL2() {
		return E_TEL2;
	}
	public void setE_TEL2(int e_TEL2) {
		E_TEL2 = e_TEL2;
	}
	public int getE_TEL3() {
		return E_TEL3;
	}
	public void setE_TEL3(int e_TEL3) {
		E_TEL3 = e_TEL3;
	}
	public String getE_ENTER_DT() {
		return E_ENTER_DT;
	}
	public void setE_ENTER_DT(String e_ENTER_DT) {
		E_ENTER_DT = e_ENTER_DT;
	}
	public String getE_RESIGN_DT() {
		return E_RESIGN_DT;
	}
	public void setE_RESIGN_DT(String e_RESIGN_DT) {
		E_RESIGN_DT = e_RESIGN_DT;
	}
	public String getE_RANK() {
		return E_RANK;
	}
	public void setE_RANK(String e_RANK) {
		E_RANK = e_RANK;
	}
	public String getE_AUTHORITY() {
		return E_AUTHORITY;
	}
	public void setE_AUTHORITY(String e_AUTHORITY) {
		E_AUTHORITY = e_AUTHORITY;
	}
	
	
}

/*
CREATE TABLE `employee` (
	`E_ID` VARCHAR(20) NOT NULL DEFAULT '' COLLATE 'latin1_swedish_ci',
	`E_PW` VARCHAR(20) NOT NULL DEFAULT '' COLLATE 'latin1_swedish_ci',
	`E_NAME` VARCHAR(20) NOT NULL DEFAULT '' COLLATE 'latin1_swedish_ci',
	`E_EMAIL1` VARCHAR(40) NOT NULL DEFAULT '' COLLATE 'latin1_swedish_ci',
	`E_EMAIL2` VARCHAR(40) NOT NULL DEFAULT '' COLLATE 'latin1_swedish_ci',
	`E_TEL1` INT(11) NOT NULL DEFAULT '0',
	`E_TEL2` INT(11) NOT NULL DEFAULT '0',
	`E_TEL3` INT(11) NOT NULL DEFAULT '0',
	`E_ENTER_DT` VARCHAR(50) NOT NULL DEFAULT '' COLLATE 'latin1_swedish_ci',
	`E_RESIGN_DT` VARCHAR(50) NOT NULL DEFAULT '' COLLATE 'latin1_swedish_ci',
	`E_RANK` VARCHAR(20) NOT NULL DEFAULT '' COLLATE 'latin1_swedish_ci',
	`E_AUTHORITY` VARCHAR(20) NOT NULL DEFAULT '' COLLATE 'latin1_swedish_ci',
	PRIMARY KEY (`E_ID`) USING BTREE
)
COLLATE='latin1_swedish_ci'
ENGINE=InnoDB
;
*/