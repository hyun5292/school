import com.spring.hamsamo.dto.MemberDTO;

@Repository
public interface MemberDAO {
	// 아이디 중복 체크
	public boolean CheckId(String mId);
	
	// 회원가입
		public boolean MJoin(Map<String, Object> map);
}
