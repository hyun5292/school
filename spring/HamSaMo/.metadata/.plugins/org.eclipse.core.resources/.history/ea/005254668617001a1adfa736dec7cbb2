package com.spring.hamsamo.dto;

import java.sql.Timestamp;

public class MemberDTO {
	private String mId = "";
	private String mName = "";
	private String mPw = "";
	private Timestamp bDate = null;
	private String mTel1 = "";
	private String mTel2 = "";
	private String mTel3 = "";
	private String mAddress1 = "";
	private String mAddress2 = "";
	
	public MemberDTO() {
		
	}
	
	public MemberDTO(String mId, String mName, String mPw, Timestamp bDate, String mTel1, String mTel2, String mTel3,
			String mAddress1, String mAddress2) {
		super();
		this.mId = mId;
		this.mName = mName;
		this.mPw = mPw;
		this.bDate = bDate;
		this.mTel1 = mTel1;
		this.mTel2 = mTel2;
		this.mTel3 = mTel3;
		this.mAddress1 = mAddress1;
		this.mAddress2 = mAddress2;
	}
	
	
}
