import java.util.Scanner;

class Challenge{
	
	
	public String readString() {
		StringBuffer sb = new StringBuffer();
		Scanner scanner = new Scanner(System.in);
		
		while(true) {
			String line = scanner.nextLine();
			if(line.length() == 1 && line.charAt(0) == ;) {
				break;
			}
			sb.append(line);
		}
		return sb.toString();
	}
}

