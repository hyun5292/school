    pageEncoding="EUC-KR"%>
    
<jsp:useBean id="stu" class="com.jsp.ex.Student" scope="page"/>
<jsp:setProperty id="stu" property="name" value="박남일"/>
<jsp:setProperty id="stu" property="major" value="융합소프트웨어과"/>
<jsp:setProperty id="stu" property="grade" value="5학년"/>
    
<!DOCTYPE html>
